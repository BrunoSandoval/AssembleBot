; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="AssembleBot"
run/main_scene="res://main_menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://cursor.png"

[autoload]

GameMaster="*res://game_master.gd"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
mouse_cursor/custom_image="res://cursor.png"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/coi_serviceworker/plugin.cfg")

[input]

tilt={
"deadzone": 0.5,
"events": []
}
egg_bind_p_explode={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":80,"key_label":0,"unicode":112,"echo":false,"script":null)
]
}

[internationalization]

locale/translations=PackedStringArray("res://localization/en.po", "res://localization/es.po")
locale/translations_pot_files=PackedStringArray("res://language/lexer.gd", "res://language/assembler.gd", "res://gameplay/gui/ErrorDisplay.gd", "res://language/parser.gd", "res://language/semantic_analyzer.gd")

[layer_names]

3d_physics/layer_1="Player"
3d_physics/layer_2="World"
3d_physics/layer_3="Interactables"
3d_physics/layer_4="Hazards"
3d_physics/layer_5="Effectors"

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/vram_compression/import_etc2_astc=true
